{
  "name": "systr",
  "productName": "systr",
  "version": "1.2.4",
  "description": "System Tracking app for windows 10.",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build-installer": "electron-builder",
    "build": "electron-builder build --win --publish never",
    "deploy": "electron-builder build --win --publish always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tylerbadger23/systr.git"
  },
  "build": {
    "appId": "system-tr",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "F:\\Electron\\systr\\icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "installerIcon": "F:\\Electron\\systr\\icon.ico",
      "uninstallerIcon": "F:\\Electron\\systr\\icon.ico",
      "uninstallDisplayName": "systr",
      "license": "readme.md",
      "allowToChangeInstallationDirectory": false
    }
  },
  "keywords": [],
  "author": {
    "name": "Tyler Laceby",
    "email": "31893030+tylerbadger23@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "/Users/tylerlaceby/Documents/systr/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "systr"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "child_process": "^1.0.2",
    "dotenv": "^8.2.0",
    "electron-json-storage": "^4.2.0",
    "electron-reload": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.3.5",
    "node-powershell": "^4.0.0",
    "os-utils": "0.0.14",
    "systeminformation": "^4.27.11"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1"
  }
}
