{
  "name": "systr",
  "productName": "systr",
  "version": "1.0.0",
  "description": "System Tracking app for windows 10.",
  "main": "src/index.js",
  "scripts": {
    "build": "electron-builder build --mac --win --publish never",
    "deploy": "electron-builder build --mac --win --publish always",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-builder build --mac --win --publish never",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build-installer": "electron-builder"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/tylerbadger23/systr.git"
  },
  "build": {
    "appId": "system-tr",
    "win": {
      "target": [
        "nsis"
      ],
      "requestedExecutionLevel": "requireAdministrator",
      "icon": "Z:/System/Electron/systr/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "installerIcon": "Z:/System/Electron/systr/icon.ico",
      "uninstallerIcon": "Z:/System/Electron/systr/icon.ico",
      "uninstallDisplayName": "systr",
      "license": "readme.md",
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [],
  "author": {
    "name": "Tyler Laceby",
    "email": "31893030+tylerbadger23@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "Z:/System/Electron/systr/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "systr"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "electron-json-storage": "^4.2.0",
    "electron-reload": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-storage": "^1.0.7",
    "electron-updater": "^4.3.5",
    "node-powershell": "^4.0.0",
    "node-wmi": "0.0.5",
    "os-utils": "0.0.14",
    "wmic-js": "0.0.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "10.1.5"
  }
}
